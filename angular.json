{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "docs": {
      "root": "apps/docs/",
      "sourceRoot": "apps/docs/src",
      "projectType": "application",
      "prefix": "fundamental-ngx",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/apps/docs",
            "index": "apps/docs/src/index.html",
            "main": "apps/docs/src/main.ts",
            "polyfills": "apps/docs/src/polyfills.ts",
            "tsConfig": "apps/docs/tsconfig.app.json",
            "assets": [
              "apps/docs/src/favicon.ico",
              "apps/docs/src/assets"
            ],
            "styles": [
              "node_modules/fundamental-styles/dist/icon.css",
              "apps/docs/src/fd-typedoc/assets/css/main.scss",
              "apps/docs/src/styles.scss"
            ],
            "scripts": [
              "node_modules/marked/lib/marked.js"
            ],
            "es5BrowserSupport": true,
            "showCircularDependencies": false
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/docs/src/environments/environment.ts",
                  "with": "apps/docs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "production-unoptimized": {
              "tsConfig": "apps/docs/tsconfig.netlify.json",
              "fileReplacements": [
                {
                  "replace": "apps/docs/src/environments/environment.ts",
                  "with": "apps/docs/src/environments/environment.prod.ts"
                }
              ],
              "aot": true,
              "extractCss": true,
              "sourceMap": false,
              "namedChunks": false,
              "vendorChunk": false,
              "optimization": false
            },
            "hmr": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "apps/docs/src/environments/environment.ts",
                  "with": "apps/docs/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "docs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "docs:build:production"
            },
            "hmr": {
              "browserTarget": "docs:build:hmr"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "docs:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/docs/tsconfig.app.json",
              "apps/docs/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/docs/**"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/docs/src/test.ts",
            "tsConfig": "apps/docs/tsconfig.spec.json",
            "karmaConfig": "apps/docs/karma.conf.js",
            "polyfills": "apps/docs/src/polyfills.ts",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/fundamental-styles/dist/fundamental-styles.css",
              "apps/docs/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/docs/src/favicon.ico",
              "apps/docs/src/assets"
            ]
          }
        }
      }
    },
    "docs-e2e": {
      "root": "apps/docs-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/docs-e2e/protractor.conf.js",
            "devServerTarget": "docs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "docs:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/docs-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**",
              "!apps/docs/**"
            ]
          }
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "prefix": "fundamental-ngx",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/core/tsconfig.lib.json",
            "project": "libs/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/core/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core/tsconfig.lib.json",
              "libs/core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/docs/**"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/core/src/test.ts",
            "tsConfig": "libs/core/tsconfig.spec.json",
            "karmaConfig": "libs/core/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
    "platform": {
      "root": "libs/platform",
      "sourceRoot": "libs/platform/src",
      "projectType": "library",
      "prefix": "fdp",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/platform/tsconfig.lib.json",
            "project": "libs/platform/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/platform/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/platform/tsconfig.lib.json",
              "libs/platform/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/platform/**"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/platform/src/test.ts",
            "tsConfig": "libs/platform/tsconfig.spec.json",
            "karmaConfig": "libs/platform/karma.conf.js"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
    "play": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/play",
      "sourceRoot": "apps/play/src",
      "prefix": "fundamental-ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/play",
            "index": "apps/play/src/index.html",
            "main": "apps/play/src/main.ts",
            "polyfills": "apps/play/src/polyfills.ts",
            "tsConfig": "apps/play/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/play/src/favicon.ico",
              "apps/play/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/fundamental-styles/dist/icon.css",
              "apps/play/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/play/src/environments/environment.ts",
                  "with": "apps/play/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "play:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "play:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "play:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/play/tsconfig.app.json",
              "apps/play/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/play/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/play/jest.config.js",
            "tsConfig": "apps/play/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/play/src/test-setup.ts"
          }
        }
      }
    },
    "play-e2e": {
      "root": "apps/play-e2e",
      "sourceRoot": "apps/play-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/play-e2e/cypress.json",
            "tsConfig": "apps/play-e2e/tsconfig.e2e.json",
            "devServerTarget": "play:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "play:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/play-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/play-e2e/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "docs",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "fd",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "fd"
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  }
}
